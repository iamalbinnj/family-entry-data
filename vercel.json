{
    "version": 2,
    "builds": [
      {
        "src": "/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/admin/(.*)", "dest": "/$1" },
      { "src": "/(.*)", "dest": "/$1" },
      { "src": "/(.*)", "dest": "/views/$1.hbs" }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/$1"
      }
    ],
    "cleanUrls": true
  }